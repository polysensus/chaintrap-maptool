    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: map generate",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/maptool/__debug__.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "args": [
                "gen",
                "--svgfile",
                "${workspaceFolder}/x.svg",
                "--loadfile",
                "${workspaceFolder}/map.json",
                "--savefile",
                "${workspaceFolder}/x.json",
                "--debug"
            ],
            "justMyCode": true
        }
    ]
}